{
  "type": "module",
  "name": "heap-sn",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "version": "1.0.0",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "fastify start app.js",
    "dev": "fastify start -w -P app.js",
