    "migrate:down": "npx knex migrate:latest --knexfile db/knexfile.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/postgres": "^5.2.0",
    "@fastify/sensible": "^5.0.0",
    "bcrypt": "^5.1.0",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.5.0",
    "fastify-plugin": "^4.0.0",
    "knex": "^2.4.2",
    "pg": "^8.9.0",
    "sqlite3": "^5.1.4"
  },
  "devDependencies": {
    "@istanbuljs/esm-loader-hook": "0.2.0",
    "tap": "^16.1.0"
  },
  "tap": {
    "node-arg": [
      "--no-warnings",
      "--experimental-loader",
      "@istanbuljs/esm-loader-hook"
    ]
  }
